package arvre;

public class Element<dado> {
	
	dado valornum;
	Element<dado> container;
	
	public dado getValornum() {
		return valornum;
	}
	public void setValornum(dado valornum) {
		this.valornum = valornum;
	}
	public Element<dado> getContainer() {
		return container;
	}
	public void setContainer(Element<dado> container) {
		this.container = container;
	}
	
	public Element(dado entrada) {
		this.valornum = entrada;
		this.container = null;
	}
}
